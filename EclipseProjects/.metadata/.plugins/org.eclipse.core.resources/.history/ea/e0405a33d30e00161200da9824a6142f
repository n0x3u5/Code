package nox.niit.books;

class Books {
	private String authorName;
	private String title;
	private double price;
	private int pages;
	Books(String authorName, String title, double price, int pages) {
		super();
		this.authorName = authorName;
		this.title = title;
		this.price = price;
		this.pages = pages;
	}
	
	
}
